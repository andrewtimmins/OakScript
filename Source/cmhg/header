command-keyword-table: module_command
    OakScript(min-args:0, max-args:1,
         invalid-syntax: "Syntax: *OakScript [<filename>]",
         help-text: "*OakScript runs an OakScript script file.\r"),
    OakScriptCompile(min-args:2, max-args:2,
         invalid-syntax: "Syntax: *OakScriptCompile <source> <output>",
         help-text: "*OakScriptCompile compiles an OakScript into bytecode.\r"),
    OakScriptRun(min-args:1, max-args:1,
         invalid-syntax: "Syntax: *OakScriptRun <bytecode_file>",
         help-text: "*OakScriptRun executes compiled OakScript bytecode.\r")

