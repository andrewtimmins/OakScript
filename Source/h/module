// Header file for OakScrpt Module
#ifndef OAKSCRPT_MODULE_H
#define OAKSCRPT_MODULE_H

#include "kernel.h"

// Module version information
#include "VersionNum"

// Function prototypes for script engine interface
extern int run_script_file(const char *filename);
extern int init_script_engine(void);
extern void cleanup_script_engine(void);

// Module entry points (generated by CMHG)
extern _kernel_oserror *module_init(const char *cmd_tail, int podule_base, void *pw);
extern _kernel_oserror *module_final(int fatal, int podule, void *pw);
extern _kernel_oserror *module_command(const char *arg_string, int argc, int cmd_no, void *pw);

// Generic veneer handlers
extern int tick_handler(_kernel_swi_regs *r, void *pw);
extern int callback_handler(_kernel_swi_regs *r, void *pw);

#endif // OAKSCRPT_MODULE_H
